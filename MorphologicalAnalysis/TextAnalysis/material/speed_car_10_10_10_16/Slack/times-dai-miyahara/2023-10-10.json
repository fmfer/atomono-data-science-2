                                        {
                                            "type": "rich_text_preformatted",
                                            "elements": [
                                                {
                                                    "type": "text",
                                                    "text": "import React, { ReactNode, MouseEvent, FC } from 'react'\n\ninterface ModalProps {\n  isOpen: boolean\n  onClose: (event: MouseEvent<HTMLButtonElement>) => void\n  children: ReactNode\n}\n\nexport const Modal: FC<ModalProps> = ({ isOpen, onClose, children }) => {\n  if (!isOpen) return null\n\n  return (\n    <div className='fixed inset-0 z-50 flex items-center justify-center'>\n      <div className='absolute inset-0 bg-black opacity-50'><\/div>\n      <div className='z-10 rounded-lg bg-white p-6 shadow-lg'>\n        {children}\n        <button onClick={onClose} className='mt-4 rounded bg-red-500 px-4 py-2 text-white'>\n          Close\n        <\/button>\n      <\/div>\n    <\/div>\n  )\n}"
                                                }
                                            ],
                                            "border": 0
                                        },
                                        {
                                            "type": "rich_text_section",
                                            "elements": [
                                                {
                                                    "type": "text",
                                                    "text": "\nwrappermodal\n"
                                                }
                                            ]
                                        },
                                        {
                                            "type": "rich_text_preformatted",
                                            "elements": [
                                                {
                                                    "type": "text",
                                                    "text": "'use client'\nimport { useState } from 'react'\nimport { Modal } from '.\/Modal'\n\nexport const ModalWrapper = () => {\n  \/\/ state\n  const [isOpen, setIsOpen] = useState(false)\n  return (\n    <>\n      <button className='button' onClick={() => setIsOpen(true)}>\n        Open Modal\n      <\/button>\n      <Modal isOpen={isOpen} onClose={() => setIsOpen(false)}>\n        <div>modal contentmodal contentmodal contentmodal contentmodal contentmodal content<\/div>\n        <div>modal content<\/div>\n        <div>modal content<\/div>\n        <div>modal content<\/div>\n        <div>modal content<\/div>\n        <div>modal content<\/div>\n        <div>modal content<\/div>\n      <\/Modal>\n    <\/>\n  )\n}"
                                                }
                                            ],
                                            "border": 0
                                        },
                                        {
                                            "type": "rich_text_section",
                                            "elements": [
                                                {
                                                    "type": "text",
                                                    "text": "このwrapperのbuttonの部分が, 作っていただいた所持品追加buttonに当たる。\nなのでwrapperコンポーネントの名前はAddInventory とかになりそう"
                                                }
                                            ]
                                        }
